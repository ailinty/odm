	<!-- 订单管理 -->
     <select id="getYDDGLBypage" resultMap="yddglResult"
		parameterClass="HashMap">
		SELECT
         TEMP1.id,dingdan_id,KeHuname,pay_way,fh_address,fh_way,
		finish_or_not,zongjine,kaidan_time,kaidanren,dingjin,yewuyuan,beizhu,zhuangtai
		FROM(
		SELECT TOP $pend$ ROW_NUMBER() OVER(ORDER BY
		kaidan_time DESC) AS ROWID,f.*,b.KeHuname as KeHuname
		FROM [sale_dingdan] as f,kehuziliao as b where  b.kehu_id=f.dingdan_name
		<dynamic prepend="where">
			<isNotEmpty property="kehuname" prepend="and">
                KeHuname like
				'%' + #kehuname# + '%'
			</isNotEmpty>
			<isNotEmpty property="dingdanhao" prepend="and">
				f.dingdan_id like
				'%' + #dingdanhao# + '%'
			</isNotEmpty>
			<isNotEmpty property="dateStartText" prepend="and">
				f.kaidan_time
				&gt;=
				#dateStartText#
			</isNotEmpty>
			<isNotEmpty property="dateEndText" prepend="and">
				f.kaidan_time &lt;=
				#dateEndText#
			</isNotEmpty>
			<isNotEmpty property="zhuangtai" prepend="and">
				f.zhuangtai =  #zhuangtai# 
			</isNotEmpty>
            <isNotEmpty property="finish_or_not" prepend="and">
                f.finish_or_not =  #finish_or_not#
            </isNotEmpty>
		</dynamic>
		
		) AS TEMP1
		WHERE ROWID >$pstart$
	</select>
	<select id="getYDDGLCount" resultClass="java.lang.Integer" 
		parameterClass="HashMap">
		select count(*) from
		[sale_dingdan] as f, kehuziliao as b where b.kehu_id=f.dingdan_name
		
		<dynamic prepend="where">
			<isNotEmpty property="kehuname" prepend="and">
				f.dingdan_name like
				'%' + #kehuname# + '%'
			</isNotEmpty>
			<isNotEmpty property="dingdanhao" prepend="and">
